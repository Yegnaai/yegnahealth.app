<YahoaScreen>
    MDNavigationLayout:
        MDScreenManager:
            MDScreen:
                BoxLayout:
                    orientation: 'vertical'
                    adaptive_height: True

                    MDBoxLayout:
                        orientation: 'horizontal'
                        adaptive_height: True
                        padding: "2dp", 0, "2dp", 0
                        pos_hint: {"center_y": .92}
                        adaptive_height: True

                        MDCard:
                            size_hint: 0.9, None  
                            height: "220dp"
                            orientation: "vertical"
                            elevation: 4
                            FitImage:
                                source: "assets/images/ye.jpg"
                                size_hint_y: None
                                height: "280dp"
                                radius: [12, 12, 0, 0]
                    MDIconButton:
                        icon: "close"
                        md_bg_color: 1, 1, 1, .6
                        x: 12
                        pos_hint: {"top": .99}
                        on_release: app.switch_to_screen('yaho')

                    ScrollView:
                        MDList:
                            padding: [10, 0, 20, 0]

                            MDCard:
                                adaptive_height: True
                                padding: "11dp"

                                MDLabel:
                                    text:("#የሳንባ ነቀርሳ(ቲቢ)")
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "15dp"


                            MDLabel:
                                text:
                                    "የሚከሰተው ማይኮባክቲሪየም ቲዩበርክሎዝስ በተባለ ባክቴሪያ ሲሆን በተለምዶ ሳንባን ይጎዳል ነገር ግን ሌሎች የሰውነት ክፍሎችንም ሊጎዳ ይችላል።"\
                                    "ቲቢ በአየር ውስጥ ይተላለፋል ንቁ ቲቢ ያለበት ሰው ሲያስል ወይም ሲያስነጥስ ባክቴሪያውን ወደ አየር በመልቀቅ በሌሎች ሊተነፍሱ ይችላሉ።\n"
                                font_name: "assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"
                            


                            MDCard:
                                adaptive_height: True
                                padding: "11dp"

                                MDLabel:
                                    text: "#ምልክቶች:-"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13dp"

                            MDLabel:
                                text:
                                    "\n"\
                                    "የቲቢ ምልክቶች ቀስ በቀስ ከበርካታ ሳምንታት ወይም ወራት በኋላ ሊዳብሩ የሚችሉ ሲሆን የማያቋርጥ ሳል፣ ትኩሳት፣ የሌሊት ላብ፣ ክብደት መቀነስ እና ድካም ያካትታሉ:-"\ 
                                    "በአንዳንድ ሁኔታዎች ቲቢ በደረት ላይ ህመም፣ ደም ማሳል እና የትንፋሽ ማጠር ሊያስከትል ይችላል።\n\n"                                    
                                font_name: "assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"

                            MDCard:
                                adaptive_height: True
                                padding: "11dp"

                                MDLabel:
                                    text: "#አጋላጭ_ሁኔታዎች:-"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13dp"
                            
                            MDLabel:
                                text:
                                    "\n"\
                                    "የቲቢ ስጋት በተጨናነቀ ወይም በቂ ንፁህ አየር በሌለበት አካባቢ መኖር ወይም መስራት እና የቲቢን በቂ ህክምና ባለማግኘት በመሳሰሉት ምክንያቶች ይጨምራል።\n\n"
                                font_name: "assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"
                            
                            MDCard:
                                adaptive_height: True
                                padding: "11dp"

                                MDLabel:
                                    text: "#መከላከያ_መንገዶች:-"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13dp"
                            
                            MDLabel:
                                text:
                                    "\n"\
                                    "ቲቢን ለመከላከል ከሁሉ የተሻለው መንገድ በ Bacillus Calmette-Guérin (BCG) ክትባት መከተብ ሲሆን ይህም ከቲቢ በሽታ መከላከያ ይሰጣል።"\
                                    "ሌሎች የመከላከያ እርምጃዎች ንቁ የቲቢ በሽታ የመያዝ ዕድላቸው ከፍተኛ በሆነባቸው ግለሰቦች ላይ የተደበቀ የቲቢ ኢንፌክሽን ምርመራን ያካትታሉ ፣ ለምሳሌ"\
                                    "ደካማ የመከላከል አቅማቸው የተዳከመ እና ድብቅ የቲቢ ኢንፌክሽን ያለባቸውን ሰዎች በኣንቲባዮቲክ ኮርስ ማከም።\n\n"
                                font_name: "assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"
                                


                            MDCard:
                                adaptive_height: True
                                padding: "11dp"

                                MDLabel:
                                    text: "#ሕክምናው"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "15dp"
                            
                            
                            MDLabel:
                                text:
                                    "\n"\
                                    "የቲቢ ሕክምና ባክቴሪያውን ለመግደል እና አንቲባዮቲክን የሚቋቋሙ ዝርያዎች እንዳይፈጠሩ ለመከላከል በተለምዶ ከ6-9 ወራት የሚወስዱ አንቲባዮቲክ መድኃኒቶችን ያካትታል።"\
                                    "መድሀኒት የሚቋቋም የቲቢ በሽታ እንዳይከሰት ለመከላከል ምልክቶቹ ቢሻሻሉም እንኳን በታዘዘው መሰረት ሙሉ የአንቲባዮቲኮችን አካሄድ ማጠናቀቅ አስፈላጊ ነው።"\
                                    "በአንዳንድ ሁኔታዎች የተበከለውን ቲሹ ከሳንባ ውስጥ ለማስወገድ ቀዶ ጥገና ሊያስፈልግ ይችላል. የቲቢ በሽታ እንዳለብዎ ከተጠራጠሩ ወዲያውኑ የሕክምና እርዳታ ማግኘት አስፈላጊ ነው።"\
                                    "ምክንያቱም ህክምናን ማዘግየት ወደ ከባድ ችግሮች እና በሽታው ወደ ሌሎች እንዲስፋፋ ያደርጋል።\n\n"
                                font_name: "assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"
                            
                            MDCard:
                                adaptive_height: True
                                padding: "11dp"

                                MDLabel:
                                    text: "#መድሃኒት ማክበር"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "15dp"
                            
                            
                            MDLabel:
                                text:
                                    "\n\n"

                                font_name:"assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"
                                adding: "20dp", 0, "20dp", 0