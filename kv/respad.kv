<RespadScreen>
    MDNavigationLayout:
        MDScreenManager:
            MDScreen:
                MDBoxLayout:
                    orientation: 'vertical'
                    padding: '10dp'

                    MDBoxLayout:
                        adaptive_height: True
                        padding: '5dp'
                        pos_hint: {"center_y": .99}

                        MDIconButton:
                            icon: "close"
                            md_bg_color: 1, 1, 1, .6
                            x: 12
                            pos_hint: {"top": .99}
                            on_release: app.switch_to_screen('respa')
                        
                        MDIconButton:
                            icon: "theme-light-dark"
                            theme_text_color: "Secondary"
                            on_release: app.toggle_theme()

                    ScrollView:
                        MDGridLayout:
                            cols: 1
                            padding: dp(2)
                            spacing: dp(2)
                            adaptive_height: True

                            MDBoxLayout:
                                orientation: 'vertical'
                                size_hint: None, None
                                size_hint_x: 0.6
                                adaptive_height: True
                                pos_hint: {"center_x": 0.5}
                                padding: dp(10)

                                FloatLayout:
                                    size_hint: None, None
                                    size: "280dp", "280dp"
                                    pos_hint: {"center_x": 0.5, "center_y": 0.5}

                                    canvas.before:
                                        Color:
                                            rgba: 1, 1, 1, 1
                                        RoundedRectangle:
                                            pos: self.pos
                                            size: self.size
                                            radius: [25, ]
                                            source: "assets/images/pos.jpg"
                                
                                MDLabel:
                                    text:
                                        "\n"\
                                        "Polycystic Ovarian Syndrome(PCOS)"
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    bold: True
                                    font_size: "20sp"
                                    theme_text_color: "Secondary"
                                    halign: 'center'
                                    adaptive_height: True
                                
                                MDLabel:
                                    text:
                                        #"\n"\
                                        #"የማኅጸን ጫፍ ካንሰር የሚጀምረው በማህፀን በር ጫፍ ሕዋሳት ውስጥ ነው ፡ ከሴት ብልት ጋር የሚገናኘው የታችኛው ክፍል። ብዙውን ጊዜ የሚከሰተው በተወሰኑ የሰዎች ፓፒሎማ ቫይረስ (HPV) በግብረ ሥጋ ግንኙነት የሚተላለፍ ኢንፌክሽን ነው።\n\n"
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    bold: True
                                    font_size: "15dp"
                                    theme_text_color: "Secondary"
                                    halign: 'center'
                                    adaptive_height: True


                                MDCard:
                                    adaptive_height: True
                                    padding: "8dp"
                                    
                                    MDLabel:
                                        text: "ምልክቶች"
                                        bold: True
                                        adaptive_height: True
                                        font_name: "assets/fonts/Abyssinica.ttf"
                                        font_size: "15dp"
                                        halign: "center"
                                        halign: "justify"
                                
                                MDLabel:
                                    text:
                                        #"\n"\ 
                                        #"በመጀመሪያዎቹ ደረጃዎች, የማኅጸን ነቀርሳ ምልክቶች የሚታዩ ምልክቶች ላይታዩ ይችላሉ። እየገፋ ሲሄድ ምልክቶቹ የሚከተሉትን ሊያካትቱ ይችላሉ-\n\n"\
                                        #"ያልተለመደ የሴት ብልት ደም መፍሰስ :- በተለይ ከግንኙነት በኋላ ፣ በወር አበባ ጊዜያት መካከል ወይም ከማረጥ በኋላ።\n\n"\
                                        #"ያልተለመደ ከሴት ብልት የምፍሰስ ፥ ውሃማ ፣ ሮዝ ወይም መጥፎ ጠረን ሊሆን ይችላል።\n\n"\
                                        #"የማያቋርጥ የዳሌ ህመም።\n\n"\
                                        #"በወሲባዊ እንቅስቃሴ ወቅት ምቾት ማጣት ወይም ደም መፍሰስ።\n\n"
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13sp"
                                    adaptive_height: True
                                    theme_text_color: "Secondary"
                                
                                MDCard:
                                    adaptive_height: True
                                    padding: "8dp"
                                    
                                    MDLabel:
                                        text: "መንስኤዎች"
                                        bold: True
                                        adaptive_height: True
                                        font_name: "assets/fonts/Abyssinica.ttf"
                                        font_size: "15dp"
                                        halign: "center"
                                        halign: "justify"
                                
                                MDLabel:
                                    text:
                                        #"\n"\ 
                                        #"HPV ኢንፌክሽን :- በጣም ጉልህ የሆነ የአደጋ መንስኤ : በተለይም 16 እና 18 ዓይነቶች።\n\n"\
                                        #"በርካታ የወሲብ አጋሮች ፡- የ (HPV) ተጋላጭነትን ይጨምራል።\n\n"\
                                        #"ማጨስ ፡- በሽታን የመከላከል አቅምን ይጎዳል፣ የካንሰር ተጋላጭነትን ይጨምራል።\n\n"\
                                        #"ደካማ የበሽታ መከላከል ስርዓት ፡- እንደ ኤች አይ ቪ ወይም የበሽታ መከላከያ መድሃኒቶች ያሉ ሁኔታዎች ተጋላጭነትን ይጨምራሉ።\n\n"\
                                        #"ቤተሰብ ታሪክ ፡- የጄኔቲክ ቅድመ-ዝንባሌ ተጋላጭነትን ሊጨምር ይችላል።\n\n"
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13sp"
                                    adaptive_height: True
                                    theme_text_color: "Secondary"
                                
                                

                                MDCard:
                                    adaptive_height: True
                                    padding: "8dp"
                                    
                                    MDLabel:
                                        text: "መከላከል"
                                        bold: True
                                        adaptive_height: True
                                        font_name: "assets/fonts/Abyssinica.ttf"
                                        font_size: "15dp"
                                        halign: "center"
                                        halign: "justify"
                                
                                MDLabel:
                                    text:
                                        #"\n"\ 
                                        #"HPV ክትባት ከፍተኛ ስጋት ካላቸው የ(HPV) ዝርያዎችን ይከላከላል።\n\n"\
                                        #"መደበኛ የማህጸን ህዋስ ምርመራ እና የ(HPV) ምርመራ።\n\n"\
                                        #"ደህንነቱ የተጠበቀ የወሲብ ተግባራት የ(HPV) ስርጭት አደጋን ይቀንሳል።\n\n"\
                                        #"ማጨስ ማቆም የካንሰር ተጋላጭነትን ይቀንሳል።\n\n"\
                                        #"ጤናማ አመጋገብ እና የአኗኗር ዘይቤ ፡- በሽታ የመከላከል ስርዓትን ያጠናክራል ኢንፌክሽኖችን ለመዋጋት ይርዳል።\n\n"
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13sp"
                                    adaptive_height: True
                                    theme_text_color: "Secondary"
                                
                                MDCard:
                                    adaptive_height: True
                                    padding: "8dp"
                                    
                                    MDLabel:
                                        text: "ትንበያ"
                                        bold: True
                                        adaptive_height: True
                                        font_name: "assets/fonts/Abyssinica.ttf"
                                        font_size: "15dp"
                                        halign: "center"
                                        halign: "justify"
                                
                                MDLabel:
                                    text:
                                        #"\n"\ 
                                        #"በቅድመ-ደረጃ ላይ ያለው የማህፀን በር ካንሰር በጊዜው በተደረገ ህክምና ከፍተኛ የመዳን እድል አለው።\n\n"\
                                        #"የማኅጸን ነቀርሳን ውጤታማ በሆነ መንገድ ለመቆጣጠር ፈጣን ምርመራ እና ጣልቃገብነት ወሳኝ ናቸው። መደበኛ የማጣሪያ ምርመራዎች እና የ(HPV) ክትባት በመከላከል ረገድ ወሳኝ ሚና ይጫወታሉ።\n\n"
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13sp"
                                    adaptive_height: True
                                    theme_text_color: "Secondary"