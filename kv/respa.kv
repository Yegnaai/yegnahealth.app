                                MDCard:
                                    size_hint: 0.45, None
                                    height: "120dp"
                                    radius: [12, 12, 12, 12]
                                    on_release: app.switch_to_screen('respaa')
                                    orientation: "vertical"
                                    elevation: 4
                                    adaptive_height: True


