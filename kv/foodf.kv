<FoodfScreen>
    MDNavigationLayout:
        MDScreenManager:
            MDScreen:
                MDBoxLayout:
                    orientation: 'vertical'
                    padding: '10dp'
                    
                    # Header Section
                    MDBoxLayout:
                        adaptive_height: True
                        #padding: '5dp'
                        pos_hint: {"center_y": .99}

                        MDLabel:
                            text: "ስፖርትና ምግቦች"
                            font_name: "assets/fonts/Yeshuah.ttf"
                            theme_text_color: "Secondary"
                            font_size: "19sp"
                            halign: 'center'
                            pos_hint: {"center_x": .5}
                            bold: True
                            adaptive_height: True
                        
                        MDIconButton:
                            icon: "close"
                            md_bg_color: 1, 1, 1, .6
                            x: 12
                            pos_hint: {"top": .99}
                            on_release: app.switch_to_screen('food')

                    # Scrollable Content Section
                    ScrollView:
                        MDGridLayout:
                            cols: 1
                            padding: dp(2)
                            spacing: dp(2)
                            adaptive_height: True
                            
                            MDBoxLayout:
                                orientation: 'vertical'
                                size_hint: None, None
