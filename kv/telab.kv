<TelabScreen>:
    MDNavigationLayout:
        MDScreenManager:
            MDScreen:
                BoxLayout:
                    orientation: 'vertical'
                    adaptive_height: True

                    MDBoxLayout:
                        orientation: 'horizontal'
                        adaptive_height: True
                        padding: "2dp", 0, "2dp", 0
                        pos_hint: {"center_y": .92}
                        adaptive_height: True

                        MDCard:
                            size_hint: 0.9, None  
                            height: "220dp"
                            orientation: "vertical"
                            elevation: 4
                            FitImage:
                                source: "assets/images/te.jpg"
                                size_hint_y: None
                                height: "280dp"
                                radius: [12, 12, 0, 0]
                    MDIconButton:
                        icon: "close"
                        md_bg_color: 1, 1, 1, .6
                        x: 12
                        pos_hint: {"top": .99}
                        on_release: app.switch_to_screen('tela')

                    ScrollView:
                        MDList:
                            padding: [10, 0, 20, 0]

                            MDCard:
                                adaptive_height: True
                                padding: "11dp"

                                MDLabel:
                                    text:("#የአእምሮ ጤና መታወክዎች")
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "15dp"


                            MDLabel:
                                text: "አንዳንድ የአእምሮ ጤና መታወክዎች ከበሽታዎቻቸው፣ ምልክቶች የአደጋ መንስኤዎች፣ የመከላከያ እርምጃዎች እና የሕክምና አማራጮች ጋር አብረው ይገኛሉ።\n"
                                font_name: "assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"
                            


                            MDCard:
                                adaptive_height: True
                                padding: "11dp"

                                MDLabel:
                                    text: "የመንፈስ ጭንቀት፡-፡"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13dp"

                            MDLabel:
                                text:
                                    "\n"\ 
                                    "የጄኔቲክ፣ የአካባቢና ባዮኬሚካላዊ ምክንያቶች ጥምረት ሊከሰት ይችላል።\n\n"\
                                    "#ምልክቶች፡-"\
                                    "የማያቋርጥ የሀዘን ስሜት ፣ የተስፋ መቁረጥ ስሜት ፣ እና ከእንቅልፍ ፣ የምግብ ፍላጎት ፣ ጉልበት እና ትኩረት ለውጥ ጋር በእንቅስቃሴዎችና ላይ ፍላጎት ወይም ደስታ ማጣት።\n\n"\
                                    "#አጋላጭ_ሁኔታዎች፡-"\
                                    "የቤተሰብ ታሪክ፣ የቀድሞ የመንፈስ ጭንቀት፣ ሥር የሰደደ ውጥረት፣ የአደንዛዥ ዕፅ አጠቃቀም እና የጤና ሁኔታዎች።\n\n"\
                                    "#መከላከያ_መንገዶች:-"\
                                    "አዘውትሮ የአካል ብቃት እንቅስቃሴ፣ ጤናማ የአመጋገብ ልማድ፣ የጭንቀት አስተዳደር ዘዴዎች እና ለጭንቀት ወይም ለአሰቃቂ ገጠመኞች እርዳታ መፈለግ።\n\n"\
                                    "#ሕክምናው:-"\
                                    "ቴራፒ፣ መድኃኒት (እንደ ፀረ-ጭንቀት ያሉ) እና የአኗኗር ዘይቤ ለውጦች።\n\n"
                                font_name: "assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"

                            MDCard:
                                adaptive_height: True
                                padding: "11dp"

                                MDLabel:
                                    text: "2. መታወክ"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13dp"
                            
                                

                            MDLabel:
                                text:
                                    "\n"\
                                    "የጄኔቲክ፣ የአካባቢ እና ባዮኬሚካላዊ ምክንያቶች ጥምረት ሊከሰት ይችላል።\n\n"\
                                    "#ምልክቶች:-"\
                                    "ስለ እለታዊ ሁኔታዎች ከመጠን በላይ መጨነቅ ወይም መፍራት ፣ እንደ ፈጣን የልብ ምት፣ ላብ እና መንቀጥቀጥ ካሉ አካላዊ ምልክቶች ጋር።\n\n"\
                                    "#አጋላጭ_ሁኔታዎች:-"\
                                    "የቤተሰብ ታሪክ፣ አሰቃቂ የህይወት ክስተቶች፣ ሥር የሰደደ ውጥረት እና የህክምና ሁኔታዎች።\n\n"\
                                    "#መከላከያ_መንገዶች:-"\
                                    "አዘውትሮ የአካል ብቃት እንቅስቃሴ፣ ጤናማ የአመጋገብ ልማድ፣ የጭንቀት አስተዳደር ዘዴዎች እና ለጭንቀት ወይም ለአሰቃቂ ገጠመኞች እርዳታ መፈለግ።\n\n"\
                                    "#ሕክምናው፡:-"\
                                    "ቴራፒ፣ መድኃኒት (እንደ ፀረ-ጭንቀት ያሉ) እና የአኗኗር ዘይቤ ለውጦች።\n\n"
                                    
                                    
                                font_name:"assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"
                                adding: "10dp", 0, "20dp", 0
                                
                            MDCard:
                                adaptive_height: True
                                padding: "11dp"

                                MDLabel:
                                    text: "#ባይፖላር ዲስኦርደር፡"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13dp"
                            
                            

                            
                            MDLabel:
                                text:
                                    "\n\n"\
                                    "የጄኔቲክ፣ የአካባቢ እና ባዮኬሚካላዊ ምክንያቶች ጥምረት ሊከሰት ይችላል።\n\n"\
                                    "#ምልክቶች:-"\
                                    "ከፍተኛ ጉልበት፣ እረፍት ማጣት እና አደገኛ ባህሪን የሚያጠቃልሉ ሁለቱም የድብርት እና እብደት ክፍሎች።\n\n"\
                                    "#አጋላጭ_ሁኔታዎች፡-"\
                                    "የቤተሰብ ታሪክ፣ የቀድሞ የባይፖላር ዲስኦርደር ክፍሎች፣ ሥር የሰደደ ውጥረት፣ የአደንዛዥ ዕፅ አጠቃቀም እና የጤና ሁኔታዎች።\n\n"\
                                    "#መከላከያ_መንገዶች፡-"\
                                    "አዘውትሮ የአካል ብቃት እንቅስቃሴ፣ ጤናማ የአመጋገብ ልማድ፣ የጭንቀት አስተዳደር ዘዴዎች እና ለጭንቀት ወይም ለአሰቃቂ ገጠመኞች እርዳታ መፈለግ።\n\n"\
                                    "#ሕክምናው፡-"\
                                    "ቴራፒ፣ መድኃኒት (እንደ ስሜት ማረጋጊያ ያሉ) እና የአኗኗር ዘይቤ ለውጦች።\n\n"\
                                    "የደም ስኳር መጠን ለመቆጣጠር የግሉኮስ መቆጣጠሪያን በመጠቀም በየጊዜው የሚደረግ ምርመራ።\n\n"
                                font_name:"assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"
                            

                            MDCard:
                                adaptive_height: True
                                padding: "11dp"

                                MDLabel:
                                    text: "#ስኪዞፈሪንያ፡"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "15dp"
                            

                            
                            MDLabel:
                                text:
                                    "\n"\
                                    "የጄኔቲክ ፣ የአካባቢ እና ባዮኬሚካላዊ ምክንያቶች ጥምረት ሊከሰት ይችላል።"\
                                    "ምልክቶች:-"\
                                    "ቅዠቶች፣ ቅዠቶች፣ የተዘበራረቀ አስተሳሰብ እና ባህሪ፣ ከማህበራዊ መራቅ እና ከስሜት ማጣት ጋር።\n\n"\
                                    "አጋላጭ_ሁኔታዎች:-"\
                                    "የቤተሰብ ታሪክ፣ ቅድመ ወሊድ ኢንፌክሽን፣ አሰቃቂ የህይወት ክስተቶች፣ የአደንዛዥ እፅ አላግባብ መጠቀም እና የህክምና ሁኔታዎች።\n\n"\
                                    "#መከላከያ_መንገዶች:-"\
                                    "የመጀመሪያ ጣልቃ ገብነት እና የሕመም ምልክቶች ሕክምና።\n\n"\
                                    "#ሕክምናው፡-"\
                                    "ቴራፒ፣ መድኃኒት እና የአኗኗር ዘይቤ ለውጦች\n\n"                                    
                                    

                                font_name:"assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"
                                adding: "20dp", 0, "20dp", 0
                            
                            MDCard:
                                adaptive_height: True
                                padding: "11dp"

                                MDLabel:
                                    text: "#መድሃኒት ማክበር"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "15dp"
                            
                            
                            MDLabel:
                                text:
                                    "\n"\
                                    "ጤናማ በሆኑ ልምዶች ላይ በማተኮር እያንዳንዱ ግለሰብ ከአእምሮ ጤና መታወክ ጋር ያለው ልምድ ልዩ ሊሆን እንደሚችል ልብ ሊባል የሚገባው ሲሆን ህክምናውም ሁልጊዜ ከፍላጎታቸው ጋር የተስማማ መሆን አለበት። ለትክክለኛ ምርመራ እና ህክምና ከጤና ባለሙያ እርዳታ መፈለግ አስፈላጊ ነው።\n\n"

                                font_name:"assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"
                                adding: "20dp", 0, "20dp", 0
                        
                    
            