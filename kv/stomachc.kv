<StomachcScreen>:
    MDNavigationLayout:
        MDScreenManager:
            MDScreen:
                MDBoxLayout:
                    orientation: 'vertical'
                    padding: '10dp'

                    MDBoxLayout:
                        adaptive_height: True
                        padding: '5dp'
                        pos_hint: {"center_y": .99}

                        MDIconButton:
                            icon: "close"
                            md_bg_color: 1, 1, 1, .6
                            x: 12
                            pos_hint: {"top": .99}
                            on_release: app.switch_to_screen('stomach')
                        MDIconButton:
                            icon: "theme-light-dark"
                            theme_text_color: "Secondary"
                            on_release: app.toggle_theme()

                    ScrollView:
                        MDGridLayout:
                            cols: 1
                            padding: dp(2)
                            spacing: dp(2)
                            adaptive_height: True

                            MDBoxLayout:
                                orientation: 'vertical'
                                size_hint: None, None
                                size_hint_x: 0.6
                                adaptive_height: True
                                pos_hint: {"center_x": 0.5}
                                padding: dp(10)

                                FloatLayout:
                                    size_hint: None, None
                                    size: "280dp", "280dp"
                                    pos_hint: {"center_x": 0.5, "center_y": 0.5}

                                    canvas.before:
                                        Color:
                                            rgba: 1, 1, 1, 1
                                        RoundedRectangle:
                                            pos: self.pos
                                            size: self.size
                                            radius: [25, ]
                                            source: "assets/images/hayp.jpg"
                            MDCard:
                                adaptive_height: True
                                padding: "11dp"
                                halign: "center"  

                                MDLabel:
                                    text: "የሄፓታይተስ በሽታ ምንድነው?"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13dp"
                                    halign: "justify"

                            MDLabel:
                                text: 
                                    "\n"\
                                    "የጉበት እብጠትን የሚያመለክት ሲሆን በቫይረስ ኢንፌክሽን፣ እንደ አልኮል ባሉ ነገሮች ሊከሰት ይችላል።"\
                                    "የቫይረስ ሄፓታይተስ በዋነኛነት በሄፐታይተስ ቫይረሶች ይከሰታል፣ በጣም የተለመደው ደግሞ ፥ ሄፓታይተስ ኤ፣ ቢ እና ፥ ሲ ናቸው።\n\n"
                                font_name: "assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"
                            

                            MDCard:
                                adaptive_height: True
                                padding: "11dp"

                                MDLabel:
                                    text: "ሄፐታይተስ ኤ"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13dp"
                                    halign: "justify"

                            MDLabel:
                                text: 
                                    "\n"\
                                    "የምተላለፈው።\n\n"\
                                    "ሄፓታይተስ ኤ አብዛኛውን ጊዜ የሚተላለፈው በበሽታው በተያዘ ሰው ሰገራ ፥ የተበከለ ምግብ በመመገብ ወይም ውሃ በመጤጣት።በቂ ያልሆነ ንፅህና ባለባቸው አካባቢዎች ከፍተኛ የመተላለፊያ ፍጥነት አላቸው።\n\n"\
                                    "ከተጠቁ ግለሰቦች ጋር የቅርብ ግንኙነት ፥ ይህ በበሽታው ከተያዘ ሰው ጋር የቤተሰብ ወይም የግብረ ሥጋ ግንኙነትን ይጨምራል።\n\n"
                                font_name: "assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"

                            
                            MDCard:
                                adaptive_height: True
                                padding: "11dp"

                                MDLabel:
                                    text: "ምልክቶች"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13dp"
                                    halign: "justify"

                            MDLabel:
                                text: 
                                    "\n"\
                                    "ለሄፐታይተስ ኤ ከተጋለጡ ከ 2-6 ባሉ ሳምንታት ውስጥ ምልከቶቹ ሊታዩ ይችላሉ እና የሚከተሉትን ሊያካትቱ ይችላሉ:-"\
                                    "አንዳንድ ሰዎች በተለይም ትንንሽ ልጆች ምልክቶች ላይታይባችው ይችላሉ ሆኖም ግን  በቫይረሱ ልያዙና ሊያስተላልፉ ይችላሉ።\n\n"\
                                    "ድካም -\n\n"\
                                    "ማቅለሽለሽ እና ማስታወክ -\n\n"\
                                    "የሆድ ህመም (በተለይ ጉበት በሚገኝበት በቀኝ በኩል)\n\n"\
                                    "ዝቅተኛ-ደረጃ ትኩሳትት\n\n"\
                                    "ጥቁር ሽንት\n\n"\
                                    "ቢጫ ቀለም (የቆዳ እና የዓይን ቢጫ መሆን)\n\n"                                
                                font_name: "assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"
                            
                            

                            MDCard:
                                adaptive_height: True
                                padding: "11dp"

                                MDLabel:
                                    text: "የመከላከያ እርምጃዎች"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13dp"
                                    halign: "justify"

                            MDLabel:
                                text: 
                                    "\n"\
                                    "ክትባት ፥  የሄፐታይተስ ኤ ክትባት በጣም ውጤታማ ነው። ከፍተኛ ተጋላጭነት ላለባቸው፣ ለምሳሌ የንፅህና አጠባበቅ ችግር ባለባቸው አካባቢዎች ለሚጓዙ ተጓዦች፣ ሥር የሰደደ የጉበት በሽታ ላለባቸው ወይም በጤና እንክብካቤ ውስጥ ለሚሠሩ ሰዎች ይመከራል።\n\n"\
                                    "ጥሩ ንጽህና ፥ እጅን በሳሙና እና በውሃ አዘውትሮ መታጠብ በተለይም መጸዳጃ ቤት ከተጠቀሙ በኋላ እና ምግብ ከመመገብ በፊት ቫይረሱን ለመከላከል ወሳኝ ነው።\n\n"\
                                    "ጤናማ ምግብ እና ውሃ ፥ ያልታከመ ውሃ ከመጠጣት እና ሄፓታይተስ ኤ በሚበዛባቸው አካባቢዎች ጥሬ ወይም ያልበሰለ ምግብ ከመብላት መቆጠብ።\n\n"\
                                    "የጉዞ ጥንቃቄዎች ፥ ያልተጣራ ውሃ እና የጎዳና ላይ ምግብን ከፍተኛ ተጋላጭነት ባለባቸው አካባቢዎች ማስወገድ።\n\n"\
                                    "ሄፓታይተስ ኤ በጥሩ ንፅህና እና በክትባት መከላከል ይቻላል፣ይህም የኢንፌክሽኑን ስርጭት ለመቀነስ ትምህርት እና ክትባቶችን ማግኘት ወሳኝ ያደርገዋል።\n\n"
                                    
                                font_name: "assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"
                        

                            MDBoxLayout:
                                orientation: 'vertical'
                                size_hint: None, None
                                size_hint_x: 0.6
                                adaptive_height: True
                                pos_hint: {"center_x": 0.5}
                                padding: dp(10)

                                FloatLayout:
                                    size_hint: None, None
                                    size: "280dp", "280dp"
                                    pos_hint: {"center_x": 0.5, "center_y": 0.5}

                                    canvas.before:
                                        Color:
                                            rgba: 1, 1, 1, 1
                                        RoundedRectangle:
                                            pos: self.pos
                                            size: self.size
                                            radius: [25, ]
                                            source: "assets/images/haypb.jpg"
                                
                                
                            MDLabel:
                                text:("ሄፓታይተስ ቢ\n\n")
                                font_name: "assets/fonts/Abyssinica.ttf"
                                bold: True
                                font_size: "20sp"
                                theme_text_color: "Secondary"
                                halign: 'center'
                                adaptive_height: True
                                
                            MDCard:
                                adaptive_height: True
                                MDLabel:
                                    text:"HBV"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13dp"
                                    halign: "justify"
                                
                            
                            MDLabel:
                                text:
                                    "\n"\
                                    "ጉበትን የሚያጠቃ የቫይረስ ኢንፌክሽን ሲሆን አጣዳፊ እና ሥር የሰደደ በሽታ ነው ማለት ይችላል።\n\n"\
                                    "በሄፕታይተስ ቢ ቫይረስ (HBV) የሚከሰት ሲሆን ይህም የጉበት እብጠት ፣ ጠባሳ እና በጊዜ ሂደት የጉበት ካንሰር ወይም cirrhosis ካልታከመ ሊያመጣ ይችላል።\n\n"\
                                    "ኤችቢቪ በጣም ተላላፊ ነው። በዋነኝነት የሚተላለፈው ከተዛማች ከሰውነት በምወጣ ፈሳሽ ጋር ንክክ ስኖረ ነው።\n\n"
                                font_name:"assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"
                                adding: "20dp", 0, "20dp", 0
                                                                                

                            MDCard:
                                adaptive_height: True
                                MDLabel:
                                    text:"መንስኤዎች"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13dp"
                                    halign: "justify"
                            
                            MDLabel:
                                text: 
                                    "\n"\
                                    "ሄፓታይተስ ቢ የሚተላለፈው ለተበከለ ደም፣ የዘር ፈሳሽ ወይም ሌሎች የሰውነት ፈሳሾች በመጋለጥ ነው። የተለመዱ የማስተላለፊያ ዘዴዎች የሚከተሉትን ያካትታሉ፥\n\n"\
                                    "ያልተጠበቀ የግብረ ሥጋ ግንኙነት ፥ በግብረ ሥጋ ግንኙነት ወቅት ይተላለፋል።\n\n"\
                                    "ከእናት ወደ ልጅ መተላለፍ ፥  የታመመች እናት በወሊድ ወቅት ኤች፥ቢ፥ቪን ለልጇ ማስተላለፍ ትችላለች።\n\n"\
                                    "መርፌዎችን መጋራት ፥ በመርፌ የሚወሰዱ መድኃኒቶችን በሚጠቀሙ ሰዎች መካከል የተለመደ።\n\n"\
                                    "በህክምና ቦታ በመርፌና በሰርጀረ ግዜ፡ የጤና ሰራተኞች ለአደጋ ሊጋለጡ ይችላሉ።\n\n"\
                                    "የተበከሉ የህክምና መሳሪያዎች አጠቃቀም ፥ ላልጸዳ መሳሪያዎች መጋለጥ የኤች፥ቢ፥ቪ።"\
                                    "ሄፓታይተስ ቢ እንደ መተቃቀፍ፣ መሳም ወይም ዕቃ መጋራት ባሉ ተራ ግንኙነት ሊተላለፍ አይችልም።\n\n"
                                font_name: "assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"

                            MDCard:
                                adaptive_height: True
                                padding: "11dp"

                                MDLabel:
                                    text: "ምልክቶች"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13dp"
                                    halign: "justify"

                            MDLabel:
                                text:
                                    "\n"\
                                    "የሄፐታይተስ ቢ ምልክቶች ከቀላል እስከ ከባድ ሊለያዩ ይችላሉ፣ እና አንዳንድ ግለሰቦች በተለይም ህጻናት ምንም ምልክት የሌላቸው ሊሆኑ ይችላሉ። ምልክቶች ሲታዩ የሚከተሉትን ሊያካትቱ ይችላሉ፥\n\n"\
                                    "ድካም - ትኩሳት\n\n"\
                                    "በመገጣጠሚያዎች እና በጡንቻዎች ላይ ህመም\n\n"\
                                    "የምግብ ፍላጎት ማጣ\n\n"\
                                    "ማቅለሽለሽ እና ማስታወክ\n\n"\
                                    "የሆድ ህመም በተለይም በጉበት አካባቢ\n\n"\
                                    "ጥቁር ሽንት እና የገረጣ ሰገራ\n\n"\
                                    "ቢጫ ቀለም (የቆዳ እና የዓይን ቢጫ) መሆን።\n\n"
                                font_name:"assets/fonts/Abyssinica.ttf"
                                font_size: "13sp"
                                adaptive_height: True
                                theme_text_color: "Secondary"
                                adding: "20dp", 0, "20dp", 0

                                                                                      
                            MDCard:
                                adaptive_height: True
                                padding: "11dp"

                                MDLabel:
                                    text: "መከላከል"
                                    bold: True
                                    adaptive_height: True
                                    font_name: "assets/fonts/Abyssinica.ttf"
                                    font_size: "13dp"
                                    halign: "justify"
                            
                            MDLabel:
                                text: 
                                    "ክትባት ፥ የሄፐታይተስ ቢ ክትባት ኢንፌክሽኑን ለመከላከል በጣም ውጤታማው መንገድ ነው። ለሁሉም ጨቅላ ህጻናት፣ ያልተከተቡ አዋቂዎች እና የጤና እንክብካቤ ሰራተኞች ይመከራል።\n\n"\
                                    "ደህንነቱ የተጠበቀ ተግባራት ፥ ኮንዶም መጠቀም፣ መርፌን ከመጋራት መቆጠብ እና ለህክምና ሂደቶች የጸዳ መሳሪያዎችን ማረጋገጥ አደጋውን ሊቀንስ ይችላል።\n\n"\
                                    "ማጣራት ፥ ነፍሰ ጡር እናቶች የኤችቢቪ ምርመራ ሊደረግላቸው ይገባል ከእናት ወደ ልጅ የመተላለፍ እድልን ይቀንሳል።\n\n"\
                                    "ሄፓታይተስ ቢ ከባድ ነገር ግን መከላከል የሚችል በሽታ ነው። ስርጭትን ለመቀነስ እና ለአደጋ የተጋለጡትን ለመከላከል ክትባት፣ መደበኛ ምርመራ እና ደህንነቱ የተጠበቀ አሰራር አስፈላጊ ናቸው።\n\n"
